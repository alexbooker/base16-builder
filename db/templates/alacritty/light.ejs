# alacritty.yml
# Name:       Base16 <%- scheme %> Light
# Author:     <%- author %>
#
# Alacritty template by sivamahadevan

colors:
  primary:
    background: '0x<%- base["07"]["hex"] %>'
    foreground: '0x<%- base["02"]["hex"] %>'

  normal:
    black:   '0x<%- base["01"]["hex"] %>'
    red:     '0x<%- base["08"]["hex"] %>'
    green:   '0x<%- base["0B"]["hex"] %>'
    yellow:  '0x<%- base["0A"]["hex"] %>'
    blue:    '0x<%- base["0D"]["hex"] %>'
    magenta: '0x<%- base["0F"]["hex"] %>'
    cyan:    '0x<%- base["0C"]["hex"] %>'
    white:   '0x<%- base["06"]["hex"] %>'

  bright:
    black:   '0x<%- base["00"]["hex"] %>'
    red:     '0x<%- base["09"]["hex"] %>'
    green:   '0x<%- base["02"]["hex"] %>'
    yellow:  '0x<%- base["03"]["hex"] %>'
    blue:    '0x<%- base["04"]["hex"] %>'
    magenta: '0x<%- base["0E"]["hex"] %>'
    cyan:    '0x<%- base["05"]["hex"] %>'
    white:   '0x<%- base["07"]["hex"] %>'
